{
  "name": "@composio/json-schema-to-zod",
  "version": "0.1.19",
  "description": "",
  "main": "src/index.ts",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.mjs",
    "types": "dist/index.d.mts"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "build": "bun run --bun tsdown",
    "test": "vitest run",
    "typecheck:src": "tsc --noEmit -p ./tsconfig.src.json",
    "typecheck:test": "tsc --noEmit -p ./tsconfig.test.json",
    "typecheck": "pnpm run typecheck:src && pnpm run typecheck:test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.0",
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.0.1",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "zod": ">=3.25.76 <5"
  }
}
