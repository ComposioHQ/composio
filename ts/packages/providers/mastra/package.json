{
  "name": "@composio/mastra",
  "version": "0.1.34",
  "description": "Agentic Provider for mastra in Composio SDK",
  "main": "src/index.ts",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run"
  },
  "keywords": [
    "composio",
    "provider",
    "mastra"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.8.0",
  "peerDependencies": {
    "@composio/core": "0.1.34",
    "@mastra/core": "^0.10.0"
  },
  "devDependencies": {
    "@composio/core": "workspace:*",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  }
}
