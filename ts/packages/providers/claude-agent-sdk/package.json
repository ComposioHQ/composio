{
  "name": "@composio/claude-agent-sdk",
  "version": "0.3.2",
  "description": "Composio provider for Claude Agent SDK",
  "main": "src/index.ts",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
