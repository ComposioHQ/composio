  "dependencies": {
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "@composio/core": "0.3.3",
    "@anthropic-ai/claude-agent-sdk": ">=0.1.0"
  },
  "devDependencies": {
    "@composio/core": "workspace:*",
    "@anthropic-ai/claude-agent-sdk": "^0.1.9",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "zod": "^4.2.1"
  }
