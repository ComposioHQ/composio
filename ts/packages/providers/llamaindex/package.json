{
  "name": "@composio/llamaindex",
  "version": "0.3.3",
  "description": "Agentic Provider for llamaindex in Composio SDK",
  "main": "src/index.ts",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run"
  },
  "keywords": [
    "composio",
    "provider",
    "llamaindex"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.0",
  "peerDependencies": {
    "@composio/core": "0.3.3",
    "llamaindex": "^0.12.0"
  },
  "devDependencies": {
    "@composio/core": "workspace:*",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
