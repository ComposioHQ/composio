# Composio API Key (Required)
COMPOSIO_API_KEY=your_composio_api_key_here

# AWS Credentials (Optional - only needed for local development)
# When running in Lambda/ECS/EC2, these are automatically provided via IAM roles
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1

# Connected Account ID (Optional - if you want to use a specific connected account)
CONNECTED_ACCOUNT_ID=your_connected_account_id
