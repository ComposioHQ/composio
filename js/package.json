    "body-parser": "^1.20.3",
    "chalk": "^4",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "express": "^4.21.2",
    "inquirer": "^10.2.2",
    "open": "^8.4.0",
    "pusher-js": "8.4.0-rc2",
    "resolve-package-path": "^4.0.0",
    "rollup-plugin-visualizer": "^5.13.1",
